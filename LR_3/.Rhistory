fotx <- function(x){
return (x^2+2*x+1)
}
SredniPryamougolnik <- function(a, b, eps, fotx ){
h=(b-a)/10000
res=0
for(i in 1:(10000- 1))
res=res+h*(fotx(a+i*h - (h/2)))
if(eps>=1)
{
return (round(res, digits=eps))
}
else
count=0
while(eps<1)
{
eps=eps*10
count=count+1
}
return (round(res,digits=count))
}
SredniPryamougolnik(9,16,0.1,fotx)
integrate(fotx,9,16)
